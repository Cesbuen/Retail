               </VBox.margin>
            </HBox>
            <HBox alignment="CENTER_RIGHT" VBox.vgrow="ALWAYS">
               <children>
                  <Button fx:id="btn_salvarProduto" mnemonicParsing="false" onAction="#salvarProduto" styleClass="Button" text="Salvar" textFill="WHITE">
                     <HBox.margin>
                        <Insets right="2.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="btn_cancelarProduto" mnemonicParsing="false" onAction="#cancelarProduto" styleClass="Button" text="Cancelar" textFill="WHITE" />
